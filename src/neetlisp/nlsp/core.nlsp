(ns 'core)

(defn complement (f)
  (fn (& args) 
      (not (apply f args))))

(defn constantly (v)
  (fn () v))

(defn identity (v) v)

(defn next (xs)
  (get xs 1))

(defn partial (f arg)
  (fn (& args)
    (apply f (cons arg args))))

(defn inc (num)
  (+ 1 num))

(defn dec (num)
  (- num 1))

"neetlisp.nlsp.core loaded"
